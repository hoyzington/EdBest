<%= form_with model: @expert, class: "" do |f| %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %>
  <%= f.label :last_name %>
  <%= f.text_field :last_name %>
  <%= f.label :email %>
  <%= f.email_field :email %>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.label :bio %>
  <%= f.text_area :bio, rows: 3 %>
  <%= f.submit "Submit" %>
<% end %>
